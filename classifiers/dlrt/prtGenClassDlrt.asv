function DLRT = prtGenClassDlrt(DataSet,Options)

Y = Y(:);
if ~DataSet.isBinary
    error('Classes must be uniquely 0 and 1');
elseif length(Y) ~= size(X,1);
    error('size mismatch');
end

DLRT.X = X;
DLRT.Y = Y;
DLRT.H1 = X(Y == 1,:);
DLRT.H0 = X(Y == 0,:);

% Place Options structure into the classifier structure
DLRT.Options = Options;
